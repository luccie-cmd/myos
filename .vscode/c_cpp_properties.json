{
    "configurations": [
        {
            "name": "GCC",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/src/kernel",
                "~/.toolchains/i686-elf/lib/gcc/i686-elf/14.1.0/include"
            ],
            "defines": [
                "DEBUG",
            ],
            "compilerPath": "~/.toolchains/i686-elf/bin/i686-elf-gcc",
            "cStandard": "c99",
            "cppStandard": "c++14",
            "intelliSenseMode": "gcc-x86",
            "compilerArgs": [
                "-std=c99",
                "-ffreestanding",
                "-nostdlib"
            ],
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}